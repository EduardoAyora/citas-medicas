{
  "name": "citas-medicas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
